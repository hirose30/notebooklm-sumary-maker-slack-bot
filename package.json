    "@slack/bolt": "^4.5.0",
    "@slack/web-api": "^7.11.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20.10.6",
    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3",
    "playwright": "^1.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
